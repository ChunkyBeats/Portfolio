<%= render('layouts/errors', :object => @project) %>

<%= form_for @project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :link %>
  <%= f.text_field :link %>
  <%= collection_check_boxes(:project, :skill_ids, Skill.all, :id, :name) %>
  <%= f.submit %>
<% end %>
